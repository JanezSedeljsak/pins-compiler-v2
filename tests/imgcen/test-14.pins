var x: int;
var memo: [100]int;

fun fib(num: int): int = ({

  if num == 0 | num == 1 then
    result = num;
  else

    if memo[num] != -1 then
      result = memo[num];
    else
      result = fib(num - 1) + fib(num - 2);
      memo[num] = result;
    end;
  end;

  result;
} where var result: int;);

fun fill_array(): void = ({
  i = 0;
  while i < 100 do
    memo[i] = -1;
    i = i + 1;
  end;
  
} where var i: int;);

fun main(): int = {
  memo[5] = 10;
  memo[5];
};